ils/update-tree.js","original":{"line":59,"column":0}},{"name":"updateTree","generated":{"line":54,"column":17},"source":"../node_modules/ipfs/src/core/components/files/utils/update-tree.js","original":{"line":59,"column":17}},{"generated":{"line":54,"column":27},"source":"../node_modules/ipfs/src/core/components/files/utils/update-tree.js","original":{"line":59,"column":0}}],"sources":{"../node_modules/ipfs/src/core/components/files/utils/update-tree.js":"'use strict'\n\nconst log = require('debug')('ipfs:mfs:utils:update-tree')\nconst addLink = require('./add-link')\n\nconst defaultOptions = {\n  shardSplitThreshold: 1000\n}\n\n// loop backwards through the trail, replacing links of all components to update CIDs\nconst updateTree = async (context, trail, options) => {\n  options = Object.assign({}, defaultOptions, options)\n\n  log('Trail', trail)\n  trail = trail.slice().reverse()\n\n  let index = 0\n  let child\n\n  for await (const node of context.ipld.getMany(trail.map(node => node.cid))) {\n    const cid = trail[index].cid\n    const name = trail[index].name\n    index++\n\n    if (!child) {\n      child = {\n        cid,\n        name,\n        size: node.size\n      }\n\n      continue\n    }\n\n    const result = await addLink(context, {\n      parent: node,\n      name: child.name,\n      cid: child.cid,\n      size: child.size,\n      flush: options.flush,\n      shardSplitThreshold: options.shardSplitThreshold,\n      hashAlg: options.hashAlg,\n      cidVersion: options.cidVersion\n    })\n\n    // new child for next loop\n    child = {\n      cid: result.cid,\n      name,\n      size: result.size\n    }\n  }\n\n  log(`Final CID ${child.cid}`)\n\n  return child.cid\n}\n\nmodule.exports = updateTree\n"},"lineCount":null}},"error":null,"hash":"ee800cf6eeb1a54f2c3fa08aee4ee7aa","cacheData":{"env":{}}}